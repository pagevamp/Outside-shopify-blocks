{{ 'ob_leadspace_2.min.css' | asset_url | stylesheet_tag }}

{% style %}
  {%- for block in section.blocks -%}
    {%- case block.type -%}
      {%- when 'leadspace_image' -%}
          {%- if section.settings.leadspace_type == 'full_bleed_desktop' -%}
            {%- if block.settings.video != blank -%}
              {%- if block.settings.show_overlay -%}
                #leadspace-{{ section.id }}::after {
                  content: '';
                  position: absolute;
                  left: 0;
                  top: 0;
                  width: 100%;
                  height: 100%;
                  background:  linear-gradient(180deg, rgba(33, 33, 33, 0.00) 43.84%, #212121 100%);
                }

                @media screen and (min-width: 768px) {
                  #leadspace-{{ section.id }}::after {
                    background:  linear-gradient(90deg, rgba(33, 33, 33, 0.00) 0%, #212121 100%);
                  }
                }
              {%- endif -%}

            {% else %}
              {%- if block.settings.full_bleed_image_mobile != blank -%}
                #leadspace-{{ section.id }} {
                  background: {%- if block.settings.show_overlay -%} linear-gradient(180deg, rgba(33, 33, 33, 0.00) 43.84%, #212121 100%), {%- endif -%} url('{{ block.settings.full_bleed_image_mobile | image_url }}') lightgray 50% / cover no-repeat;
                }
                {% else %}
